<%= render 'books/side'%>
	<div class="col-xs-9">
<h2>Users</h2>
<table class="table table-hover">
	<thead>
		<tr>
			<th>image</th>
			<th>name</th>
			<th></th>
			<th colspan="2"></th>
		</tr>
	</thead>
	<tbody>
		<% @search_users.each do |f| %>
		<tr>
			<td><%= attachment_image_tag f, :profile_image, :fill, 40, 40, format: 'jpeg', class: "pull-left profile-img", fallback: "no_image.jpg",size:'40x40'%></td>
			<td><%= f.name %></td>
		</tr>
		 <% end %>
	</tbody>
</table>
</div>
