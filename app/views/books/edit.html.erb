<div class="container">
	<h1>eding book</h1>
	<%= form_for(@book) do |f| %>
	<p><%= f.label :title %></p>
	<p><%= f.text_field :title %></p>
	<p><%= f.label :body %></p>
	<p><%= f.text_area :body %></p>
	<p><%= f.submit 'Update Book' %></p>
	<%= link_to'show',book_path(@book.id) %> /
	<%= link_to'Back',books_path,class:"fas fa-pastafarianism" %>
	<div class="past">
		<i class="fas fa-pastafarianism"></i>
	</div>
	<% end %>
</div>